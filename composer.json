{
    "name": "d3jn/vizcache",
    "authors": [
        {
            "name": "Serhii Yaniuk",
            "email": "serhiiyaniuk@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/container": "~5.6",
        "illuminate/console": "~5.6",
        "illuminate/filesystem": "~5.6",
        "illuminate/support": "~5.6",
        "illuminate/cache": "~5.6",
        "illuminate/contracts": "~5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "D3jn\\Vizcache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "D3jn\\Vizcache\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "D3jn\\Vizcache\\VizcacheServiceProvider"
            ],
            "aliases": {
                "Vizcache": "D3jn\\Vizcache\\Facades\\Vizcache"
            }
        }
    },
    "license": "MIT"
}
